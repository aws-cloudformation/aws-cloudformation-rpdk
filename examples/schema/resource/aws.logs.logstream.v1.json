{
    "$id": "aws.logs.logstream.v1.json",
    "typeName": "AWS::Logs::LogStream",
    "properties": {
        "LogGroupName": {
            "$ref": "aws.logs.loggroup.v1.json#/properties/LogGroupName"
        },
        "LogStreamName": {
            "type": "string",
            "pattern": "[^:*]*",
            "minLength": 1,
            "maxLength": 512
        }
    },
    "required": [
        "LogGroupName"
    ],
    "additionalProperties": false
}

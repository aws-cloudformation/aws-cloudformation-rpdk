{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "An S3 Bucket",
    "Resources": {
        "S3Bucket": {
            "Type": "AWS::S3::Bucket",
            "DeletionPolicy": "Retain",
            "Properties": {
                "VersioningConfiguration": {
                    "Status": "Enabled"
                }
            }
        }
    }
}
